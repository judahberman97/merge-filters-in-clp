{% comment %}
    Renders an SVG social media icon.

    Accepts:
    - social_icon {String} name of the social media icon you'd like to render.
    - icons_shape {String} accepts 'square', 'circle' or 'floating'.
    - icons_size {Number} width/height of the svg icon in pixels.
    - icons_classes {String} classes for icons.
	- original_icons {Boolean} when true, it renders the original icons with their brand colors. Default to false.
    - no_animation: {Boolean} set to true to disable hover animations

    Usage:
    {% render 'armament-social-icons' with social_icon: 'facebook', icons_shape: 'square', icons_classes: 'text-page bg-body' %}

{% endcomment %}

{% liquid
    unless no_animation
        assign icon_hover_classes = ' icon-hover-classes'
    endunless

    assign icons_classes = icons_classes | append: icon_hover_classes

    if icons_shape == 'circle'
        assign shape = 'rounded-full overflow-hidden '
    endif

    if icons_size == blank
        assign icons_size = 16
    endif
    assign bg_classes = 'w-[44px] h-[44px] flex justify-center items-center ' | append: shape

    assign original_icons = original_icons | default: false
%}

{% case social_icon %}
    {% when 'facebook' %}
        <div class="{{ bg_classes }}{{ icons_classes }}" {% if original_icons and icons_shape == 'floating' %}style="background-color: transparent;"{% elsif original_icons and icons_shape != 'floating' %}style="background-color: #3B5998;"{% endif %}>
            <svg width="{{ icons_size }}" height="{{ icons_size }}" viewBox="0 0 31 30" fill="none" xmlns="http://www.w3.org/2000/svg">
